	                              <option value="{{pencarikerja.jenis_kelamin}}">{{pencarikerja.jenis_kelamin}}</option>
	                              {% for i in
	                                [
	                                'L','P'
	                                ] %}
	                                <option value="{{ i }}" {% if requestParams['jenis_kelamin'] == i %}selected{% endif %}>{{i }}</option>
	                                {% endfor %}
	                            </select>
	                            <span class="help-block">{{ error('jenis_kelamin') }}</span> -->
	                        </div>

							<div class="form-group{% if has_error('no_kartu_keluarga') %} has-error{% endif %}">
	                            <label for="">Provinsi</label>
	                            <input class="form-control" value="{{pencarikerja.provinsi.lokasi_nama}}">
	                        </div>

							<div class="form-group{% if has_error('no_kartu_keluarga') %} has-error{% endif %}">
	                            <label for="">Kabupaten / Kota </label>
	                            <input class="form-control" value="{{pencarikerja.kabupatenkota.lokasi_nama}}">
	                        </div>

							<div class="form-group{% if has_error('no_kartu_keluarga') %} has-error{% endif %}">
	                            <label for="">Kecamatan </label>
	                            <input class="form-control" value="{{pencarikerja.kecamatan.lokasi_nama}}">
	                        </div>

							<div class="form-group{% if has_error('no_kartu_keluarga') %} has-error{% endif %}">
	                            <label for="">Kelurahaan </label>
	                            <input class="form-control" value="{{pencarikerja.kelurahan.lokasi_nama}}">
	                        </div>

	                        <div class="form-group">
								<label for="alamat">Alamat pencarikerja</label>
								<textarea name="alamat" class="form-control" id="alamat" placeholder="{{pencarikerja.alamat}}" rows="3"></textarea>
							</div>

